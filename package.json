{
    "name": "ruby-markers",
    "displayName": "Ruby Markers",
    "description": "Evalute Ruby code in the editor just typing # =>",
    "version": "0.0.1",
    "publisher": "artero",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onCommand:extension.evaluate",
        "onCommand:extension.add_mark"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
          {
            "command": "extension.sayHello",
            "title": "Ruby Markers: Hello World"
          },
          {
            "command": "extension.evaluate",
            "title": "Ruby Markers: Evaluate Marks"
          },
          {
            "command": "extension.add_mark",
            "title": "Ruby Markers: Add Mark"
          }
        ],
        "keybindings":[
            {
                "command": "extension.evaluate",
                "key": "cmd+ctrl+e"
            },
            {
                "command": "extension.add_mark",
                "key": "cmd+ctrl+m"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}